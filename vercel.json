{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://real-state-api-mocha.vercel.app"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ]
}
